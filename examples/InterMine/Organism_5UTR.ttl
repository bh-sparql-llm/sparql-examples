@prefix ex: <https://www.intermine.org/.well-known/sparql-examples/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <https://schema.org/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
ex:Organism_5UTR a sh:SPARQLExecutable,
	sh:SPARQLSelectExecutable ;
	rdfs:comment "Show all 5' UTRs for a particular organism."@en ;
	sh:prefixes _:sparql_examples_prefixes ;
	sh:select """PREFIX intermine_voc: <http://bio2rdf.org/intermine_voc:>
SELECT
	?utrs_id_primaryidentifier
	?utrs_id_length
	?chromosomelocation_id_start
	?chromosomelocation_id_end
	?gene_id_secondaryidentifier
WHERE {
	?gene_id a intermine_voc:Gene .
	?gene_id	intermine_voc:UTRs_id	?utrs_id	.
	?utrs_id	intermine_voc:primaryIdentifier	?utrs_id_primaryidentifier	.
	?utrs_id	intermine_voc:length	?utrs_id_length	.
	?utrs_id	intermine_voc:chromosomeLocation_id	?chromosomelocation_id	.
	?chromosomelocation_id	intermine_voc:start	?chromosomelocation_id_start	.
	?chromosomelocation_id	intermine_voc:end	?chromosomelocation_id_end	.
	?gene_id	intermine_voc:secondaryIdentifier	?gene_id_secondaryidentifier	.
	FILTER (
		?organism_id_name  = 'Drosophila melanogaster'
	)
}
ORDER BY ASC(?gene_id_utrs_id_primaryidentifier)
""" ;
    schema:target <http://intermine.bio2rdf.org:7000/flymine/IudBrP> .
