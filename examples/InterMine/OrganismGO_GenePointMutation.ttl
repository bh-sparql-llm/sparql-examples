@prefix ex: <https://www.intermine.org/.well-known/sparql-examples/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <https://schema.org/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
ex:OrganismGO_GenePointMutation a sh:SPARQLExecutable,
	sh:SPARQLSelectExecutable ;
	rdfs:comment "Show all D. melanogaster genes that have been annotated with a particular GO term and the point mutations which overlap these genes. (Data Source: GO Consortium, FlyBase)."@en ;
	sh:prefixes _:sparql_examples_prefixes ;
	sh:select """PREFIX intermine_voc: <http://bio2rdf.org/intermine_voc:>
SELECT
	?gene_id_secondaryidentifier
	?gene_id_symbol
	?overlappingfeatures_id_primaryidentifier
	?datasource_id_name
	?ontologyterm_id_identifier
	?ontologyterm_id_name
	?parents_id_name
WHERE {
	?gene_id a intermine_voc:Gene .
	?gene_id	intermine_voc:secondaryIdentifier	?gene_id_secondaryidentifier	.
	?gene_id	intermine_voc:symbol	?gene_id_symbol	.
	?gene_id	intermine_voc:overlappingFeatures_id	?overlappingfeatures_id	.
	?overlappingfeatures_id	intermine_voc:primaryIdentifier	?overlappingfeatures_id_primaryidentifier	.
	?overlappingfeatures_id	intermine_voc:dataSets_id	?datasets_id	.
	?datasets_id	intermine_voc:dataSource_id	?datasource_id	.
	?datasource_id	intermine_voc:name	?datasource_id_name	.
	?gene_id	intermine_voc:goAnnotation_id	?goannotation_id	.
	?goannotation_id	intermine_voc:ontologyTerm_id	?ontologyterm_id	.
	?ontologyterm_id	intermine_voc:identifier	?ontologyterm_id_identifier	.
	?ontologyterm_id	intermine_voc:name	?ontologyterm_id_name	.
	?ontologyterm_id	intermine_voc:parents_id	?parents_id	.
	?parents_id	intermine_voc:name	?parents_id_name	.
	FILTER (
		?organism_id_name  = 'Drosophila melanogaster'
	)
	FILTER (
		?parents_id_name  = 'gap junction channel activity'
	)
}
ORDER BY ASC(?gene_id_secondaryidentifier)
""" ;
    schema:target <http://intermine.bio2rdf.org:7000/flymine/IudBrP> .
